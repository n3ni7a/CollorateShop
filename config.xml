<?xml version='1.0' encoding='utf-8'?>
<widget id="com.giovanny.precios" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Precios de Lista</name>
    <description>
        AAplicaci√≥n movil para visualizar los precios de cada uno de los productos de los catalogos de Price Shoes.
    </description>
    <author email="n3ni7a89@gmail.com" href="http://programas-prueba.000webhostapp.com/codigo_barras/">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
</widget>
